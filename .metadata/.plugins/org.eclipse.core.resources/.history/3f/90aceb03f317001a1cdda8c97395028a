
public class ц╟ещеепР {
	public static void sort(int[] a)
	{
		A:
		while(true)
		{	boolean sorted=true;
			for(int i=0;i<a.length-1;i++)
			{
				if(a[i]>a[i+1])
				{
					int k=a[i];
					a[i]=a[i+1];
					a[i+1]=k;
					sorted=false;
					break;	
				}
			}
			if(sorted==true)
				break A;
		}
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
			int[] a= {2,4,6,8,3,5};
			sort(a);
			for(int i=0;i<a.length;i++)
			{
				System.out.println(a[i]);
			}
	}

}
